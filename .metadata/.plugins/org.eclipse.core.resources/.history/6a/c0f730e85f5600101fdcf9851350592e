package business.model;

public class Examen {
	private String data;
	private String tip;
	private Nota nota;
	
	public Examen(Examen e){
		data=e.data;
		tip=e.tip;		
		nota=e.nota;
	}
	public Examen(String d, String t, Nota n){
		data=d;
		tip=t;
		nota=n;
	}
	public Examen(String d, String t){
		data=d;
		tip=t;
		nota=0;
	}
	public void setData(String d){
		data=d;
	}
	public void setNota(Nota n){
		nota=n;
	}
	public void setTip(String t){
		tip=t;
	}
	public String getData(){
		return data;
	}
	public Nota getNota(){
		return nota;
	}
	public String getTip(){
		return tip;
	}
}
